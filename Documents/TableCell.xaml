             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Berry.DocxViewer.Documents"
             mc:Ignorable="d">
    <Border Name="UILeftBorder" BorderThickness="1,0,0,0" BorderBrush="Black">
        <Border Name="UITopBorder" BorderThickness="0,1,0,0" BorderBrush="Black">
            <Border Name="UIRightBorder" BorderThickness="0,0,1,0" BorderBrush="Black">
                <Border Name="UIBottomBorder" BorderThickness="0,0,0,1" BorderBrush="Black">
                    <StackPanel Name="UIContent">

                    </StackPanel>
                </Border>
            </Border>
        </Border>
    </Border>
    
</UserControl>
